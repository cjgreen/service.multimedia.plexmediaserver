<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>URL</key>
	<dict>
		<key>SoundCloud</key>
		<dict>
			<key>URLPatterns</key>
			<array>
				<string>^(?!.*(/explore/|/sets/|/messages/))https?://soundcloud\.com/[^/]+/[^/]+$</string>
				<string>^https?://api\.soundcloud\.com/tracks/\d+/stream</string>
			</array>
			<key>TestURLs</key>
			<array>
				<string>https://soundcloud.com/soundcloud/new-soundcloud-stats</string>
			</array>
		</dict>
	</dict>
</dict>
</plist>
